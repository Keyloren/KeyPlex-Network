			else if arg-1 is "admin":
				if arg-2 is set:
					set {mineplex.rank.%arg-2%} to "&4&lADMIN "
					set {_mineplex.setadmin.temp} to "%arg-2%" parsed as player
					execute console command "/__mp__dp__ %arg-2%"
